@CHARSET "UTF-8";

body {
	padding-top: 70px;
	background-color: #F4F4EE;
	color: #494949;
	margin-bottom: 20px;
}

* { -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box; }
.clear { clear: both; }

/* Typography */
body { font-family:'Open Sans', arial, sans-serif; font-size:16px; }
h1,h2,h3,h4,h5,h6 { font-family:'Open Sans', arial, sans-serif; font-size: 21px;}
a { color: #2FB795; }
a:hover { color: #2BA083; }

.inline {display: inline;}

input[type="checkbox"] {
    position: relative;
    bottom: 1px;
    height: auto !important;
}

form label.error {
    width: 100%;
    color: #b72424;
    font-weight: 400;
    padding-left: 20px;
    margin-top: 2px;
}

.table {
	border-left: medium none;
	border-right: medium none;
	margin-bottom: 5px;
}

.table tr {
	border-top: medium none;
}

.table tr.show-hover:hover {
	background-color: #FFFFFF !important;
	/*
	box-shadow: 0 0 2px 2px rgba(127, 127, 127, 0.5);
	-webkit-box-shadow: 0 0 2px 2px rgba(127, 127, 127, 0.5);
	-moz-box-shadow: 0 0 2px 2px rgba(127, 127, 127, 0.5);
	*/
}

table tr th a {
    color: #5f5f5f;
}

table tr th a:hover {
	text-decoration: none;
	color: #5f5f5f;
}

.table td {
	vertical-align: middle !important;
}

.table-control {
	color: #2FB795;
	display: inline;
	cursor: pointer;
	font-size: 14px;
}

.t-table {
	display: table;
}

.t-row {
    display: table-row;
}

.t-row:hover {
	background-color: #ffffff;
}

.t-cell {
    display: table-cell;
    padding: 0 10px 0 0;
}

label {
	margin-bottom: 0;
	margin-top: 8px;
}

input.inline { padding: 2px 6px !important; height: 25px !important; }
input:focus, input:active { outline: none !important; }

.file-input { height: 34px !important; padding: 6px 12px !important; }

select.inline { height: 25px !important; padding: 2px 6px !important; }

.logo-wrap { text-align: center; margin-bottom: 20px; }
.logo-wrap img { width: 375px; }
.login-wrap { max-width: 250px; margin: 0 auto; }
.login-wrap input { width: 100%; }
.login-wrap button { margin-top: 10px; }

.navbar { 
	background-color: #D3D3CD;
	height: 50px;
	border-width: 0;
	box-shadow: 0 0 8px 2px rgba(127, 127, 127, 0.9);
	-webkit-box-shadow: 0 0 8px 2px rgba(127, 127, 127, 0.9);
	-moz-box-shadow: 0 0 8px 2px rgba(127, 127, 127, 0.9);
}
.navbar-nav { border-left: 1px solid #A8A8A8 }
.navbar-nav > li > a { color: #494949 !important; height: 50px; line-height: 20px;}

.navbar-nav > .active > a:hover, .navbar-nav > .active > a:focus {
	color: #494949 !important;
	background-color: #A8A8A8;
}

.navbar-nav > .active > a {
	color: #494949 !important;
	background-color: #A8A8A8;
}

.navbar-brand {
	color: #494949 !important;
}

.navbar > a,
.navbar-nav > a {
	color: #494949 !important;
}

#logoutBtn {
    margin-right: 10px;
}

